<?xml version="1.0" encoding="UTF-8"?> 
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
      <bean id="employee1" class="com.demo.Employee">
      	<property name="empId" value="10"/>
      	<property name="empName" value="Rohan"/>
      	<property name="empAddress">
      		<list>
      			<value>143-Parallex WorkSpaces</value>
      			<value>Sector-63</value>
      			<value>Noida</value>
      		</list>
      	</property>
      </bean>
      
      <!-- Auto Wiring -->
	  <bean id="great1" class="com.demo.Greeting1">
	  		<property name="i" value="10"/>
	  </bean>
	  <bean id="g1" class="com.demo.Greeting1">
	  		<constructor-arg name="i" value="20"/>
	  </bean>
      <bean id="g" class="com.demo.Greeting" autowire="constructor"/>
      
      <!-- Factory Method -->
      <bean id="a1" class="com.factoryMethod.A"/>
      <bean id="a" class="com.factoryMethod.A" factory-method="getA" factory-bean="a1"></bean>  
      
</beans>