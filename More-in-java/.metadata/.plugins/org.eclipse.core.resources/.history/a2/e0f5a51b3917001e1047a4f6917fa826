server:
  port: 8084
  
spring:
  application:
    name: apigateway
  cloud:
    gateway:
      routes:
      - id: userservice
        uri: lb://userservice
        predicates:
        - Path=/users/**
    
eureka:
  instance:
    prefer-ip-address: true
  client:
    fetch-registry: true
    register-with-eureka: true
    service-url:
      defaultZone: http://localhost:8761/eureka